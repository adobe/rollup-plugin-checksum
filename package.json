{
  "name": "@trieloff/rollup-plugin-checksum",
  "description": "Rollup plugin that emits a file containing the checksum of all emitted chunks and assets.",
  "version": "1.1.0",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "test": "rollup --config test/rollup.config.js",
    "test:sri": "rollup --config test/rollup.config.sri.js",
    "test:sri:multi": "rollup --config test/rollup.config.sri-multi.js",
    "verify:sri": "npm run test:sri && node ./scripts/verify-sri.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trieloff/rollup-plugin-checksum.git"
  },
  "author": {
    "name": "Andi PÃ¤tzold",
    "email": "github@andipaetzold.com",
    "url": "https://github.com/andipaetzold"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trieloff/rollup-plugin-checksum/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.1.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@tsconfig/node12": "1.0.7",
    "glob": "^7.1.7",
    "rollup": "2.43.0",
    "semantic-release": "17.4.2",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "rollup": ">=1.26.0"
  },
  "dependencies": {
    "@types/node": "~14.14.41"
  }
}
